{
  "name": "cdk-athena",
  "description": "CDK Construct for creating Athena resources",
  "version": "3.0.0",
  "license": "Apache-2.0",
  "author": {
    "name": "Daniel Schroeder",
    "url": "https://www.udondan.com/",
    "twitter": "udondan"
  },
  "awscdkio": {
    "twitter": "udondan"
  },
  "homepage": "https://github.com/udondan/cdk-athena",
  "repository": {
    "type": "git",
    "url": "https://github.com/udondan/cdk-athena.git"
  },
  "bugs": {
    "url": "https://github.com/udondan/cdk-athena/issues"
  },
  "keywords": [
    "aws",
    "cdk",
    "aws-cdk",
    "athena",
    "workgroup",
    "namedquery"
  ],
  "stability": "stable",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "jsii",
    "build:watch": "jsii -w",
    "package": "jsii-pacmak",
    "prepackage": "jsii && lambda/build"
  },
  "jsii": {
    "excludeTypescript": [
      "lambda"
    ],
    "outdir": "dist",
    "targets": {
      "python": {
        "distName": "cdk-athena",
        "module": "cdk_athena"
      }
    }
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.97",
    "@types/js-yaml": "^4.0.5",
    "@types/node": "^20.1.0",
    "aws-cdk-lib": "^2.0.0",
    "aws-lambda": "^1.0.7",
    "aws-sdk": "^2.1134.0",
    "constructs": "^10.0.0",
    "jsii": "^5.0.0",
    "jsii-pacmak": "^1.58.0",
    "ts-node": "^10.7.0",
    "typescript": "^5.0.2",
    "aws-cloudformation-custom-resource": "^3.1.0"
  },
  "peerDependencies": {
    "aws-cdk-lib": "^2.0.0",
    "constructs": "^10.0.0"
  }
}
