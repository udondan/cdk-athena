{
  "name": "cdk-athena",
  "description": "CDK Construct for creating Athena resources",
  "version": "3.0.0",
  "license": "Apache-2.0",
  "author": {
    "name": "Daniel Schroeder",
    "url": "https://www.udondan.com/",
    "twitter": "udondan"
  },
  "awscdkio": {
    "twitter": "udondan"
  },
  "homepage": "https://github.com/udondan/cdk-athena",
  "repository": {
    "type": "git",
    "url": "https://github.com/udondan/cdk-athena.git"
  },
  "bugs": {
    "url": "https://github.com/udondan/cdk-athena/issues"
  },
  "keywords": [
    "aws",
    "cdk",
    "aws-cdk",
    "athena",
    "workgroup",
    "namedquery"
  ],
  "stability": "stable",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "jsii",
    "build:watch": "jsii -w",
    "package": "jsii-pacmak",
    "prepackage": "jsii && lambda/build"
  },
  "jsii": {
    "excludeTypescript": [
      "lambda"
    ],
    "outdir": "dist",
    "targets": {
      "python": {
        "distName": "cdk-athena",
        "module": "cdk_athena"
      }
    }
  },
  "devDependencies": {
    "@aws-sdk/client-athena": "3.540.0",
    "@swc/core": "1.4.8",
    "@swc/helpers": "0.5.7",
    "@types/js-yaml": "4.0.9",
    "@types/node": "20.11.30",
    "@typescript-eslint/eslint-plugin": "7.3.1",
    "@typescript-eslint/parser": "7.3.1",
    "aws-cdk-lib": "2.133.0",
    "constructs": "10.3.0",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-deprecation": "2.0.0",
    "eslint-plugin-prettier": "5.1.3",
    "jsii": "5.3.29",
    "jsii-pacmak": "1.96.0",
    "prettier": "3.2.5",
    "publib": "0.2.809",
    "regenerator-runtime": "0.14.1",
    "ts-node": "10.9.2",
    "typescript": "5.4.3",
    "aws-cloudformation-custom-resource": "5.0.0"
  },
  "peerDependencies": {
    "aws-cdk-lib": "^2.0.0",
    "constructs": "^10.0.0"
  }
}
